<!DOCTYPE html>
<html>
<head>
  <style>
    body { font-family: Arial, sans-serif; }
    h1 { color: #333; }
    table { width: 100%; border-collapse: collapse; }
    table, th, td { border: 1px solid black; }
    th, td { padding: 8px; text-align: left; }
  </style>
</head>
<body>
  <h1>{{title}}</h1>  <!-- Dynamically rendered title -->
  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Age</th>
        <th>ID</th>
        <th>Date of Birth</th>
        <th>Gender</th>
      </tr>
    </thead>
    <tbody>
      {{#each users}}  <!-- Iterate over users -->
        <tr>
          <td>{{this.name}}</td>      
          <td>{{this.age}}</td>
          <td>{{this.id}}</td>
          <td>{{this.date_of_birth}}</td>
          <td>{{this.gender}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</body>
</html>
